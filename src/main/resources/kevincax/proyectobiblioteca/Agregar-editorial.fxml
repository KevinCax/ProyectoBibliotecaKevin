            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="AGREGAR EDITORIALES" textAlignment="CENTER">
               <font>
                  <Font name="Arial Italic" size="32.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <TextField fx:id="textNombreEditorial" layoutX="70.0" layoutY="137.0" promptText="Nombre de la Editorial" />
      <TextField fx:id="textFieldDireccion" layoutX="339.0" layoutY="137.0" promptText="Dirección" />
      <Button fx:id="guardarEditoriales" layoutX="95.0" layoutY="306.0" mnemonicParsing="false" onAction="#guardarEditoriales" prefHeight="52.0" prefWidth="137.0" style="-fx-background-color: #0598FF;" text="GUARDAR" textFill="WHITE">
         <font>
            <Font name="Arial Bold Italic" size="15.0" />
